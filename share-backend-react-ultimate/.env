PORT=8081

#mật khẩu mặc định khi tạo fake data cho user
MIGRATION_USER_PASSWORD=123456

#connect database
DB_HOST=localhost
#default: 3306 với mysql
DB_PORT=3306 
DB_DATABASE_NAME=hoidanit
DB_USERNAME=root
DB_PASSWORD=123456

#dialect: /* one of 'mysql' | 'postgres' | 'sqlite' | 'mariadb' | 'mssql' | 'db2' | 'snowflake' | 'oracle' */
DB_DIALECT=mysql


#run at localhost, set ssl to false; run on heroku, set ssl to true
DB_SSL=false

#jwt
JWT_SECRET=jwtsecret
#token hết hạn sau 365 ngày
JWT_EXPIRED=365d
